{
  "name": "@mojiee/utils",
  "version": "0.3.3",
  "description": "Utility function",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    },
    "./date": {
      "import": "./dist/date.mjs",
      "require": "./dist/date.cjs",
      "types": "./date.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "author": "Mojie",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "prepare": "husky install",
    "dev": "npm run build -- --watch",
    "build": "rimraf dist && rollup -c",
    "release": "npx bumpp --commit --push --tag --no-verify",
    "docs": "typedoc",
    "docs:html": "npm run docs -- --out docs-html --plugin typedoc-plugin-missing-exports",
    "lint:eslint": "eslint --ext .ts . --fix",
    "lint:prettier": "prettier --write \"**/*.{ts,json,md}\"",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "bugs": {
    "url": "https://github.com/jayfongs/utils/issues"
  },
  "homepage": "https://github.com/jayfongs/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfongs/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "*.d.ts"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-json": "^4.1.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "dayjs": "^1.11.0",
    "esbuild": "^0.14.34",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.9.1",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.14",
    "typedoc-plugin-markdown": "^3.11.14",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "^4.6.3"
  }
}
