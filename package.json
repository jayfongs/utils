{
  "name": "@mojiee/utils",
  "type": "module",
  "version": "0.6.0",
  "description": "Utility function",
  "author": "Mojie",
  "license": "MIT",
  "homepage": "https://github.com/jayfongs/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfongs/utils.git"
  },
  "bugs": {
    "url": "https://github.com/jayfongs/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./date": {
      "types": "./date.d.ts",
      "require": "./dist/date.cjs",
      "import": "./dist/date.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "prepare": "husky install",
    "dev": "npm run build -- --watch",
    "build": "rimraf dist && rollup -c",
    "release": "npx bumpp --commit --push --tag --no-verify",
    "docs": "typedoc",
    "docs:html": "npm run docs -- --out docs-html --plugin typedoc-plugin-missing-exports",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.18",
    "@types/throttle-debounce": "^5.0.0",
    "dayjs": "^1.11.7",
    "esbuild": "^0.16.17",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "throttle-debounce": "^5.0.0",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.1"
  }
}
