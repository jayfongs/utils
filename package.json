{
  "name": "@mojiee/utils",
  "version": "0.1.1",
  "description": "Utility function",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Mojie",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "prepare": "husky install",
    "dev": "npm run build -- --watch",
    "build": "rimraf dist && tsup src/index.ts src/date.ts --format esm,cjs --dts",
    "release": "npx bumpp --commit --push --tag --no-verify",
    "docs": "typedoc",
    "docs:html": "npm run docs -- --out docs-html --plugin typedoc-plugin-missing-exports",
    "lint:eslint": "eslint --ext .ts . --fix",
    "lint:prettier": "prettier --write \"**/*.{ts,json,md}\"",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "bugs": {
    "url": "https://github.com/jayfongs/utils/issues"
  },
  "homepage": "https://github.com/jayfongs/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfongs/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "dayjs": "^1.10.8",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tsup": "^5.11.13",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "^4.6.2"
  }
}
