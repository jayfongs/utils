{
  "name": "@mojiee/utils",
  "type": "module",
  "version": "0.6.1",
  "description": "Utility function",
  "author": "Mojie",
  "license": "MIT",
  "homepage": "https://github.com/jayfongs/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfongs/utils.git"
  },
  "bugs": {
    "url": "https://github.com/jayfongs/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./date": {
      "types": "./date.d.ts",
      "require": "./dist/date.cjs",
      "import": "./dist/date.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "prepare": "husky install",
    "dev": "npm run build -- --watch",
    "build": "rimraf dist && rollup -c",
    "release": "npx bumpp --commit --push --tag --no-verify",
    "docs": "typedoc",
    "docs:html": "npm run docs -- --out docs-html --plugin typedoc-plugin-missing-exports",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^20.3.1",
    "@types/throttle-debounce": "^5.0.0",
    "dayjs": "^1.11.8",
    "esbuild": "^0.18.4",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "throttle-debounce": "^5.0.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
