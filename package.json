{
  "name": "@mojiee/utils",
  "version": "0.5.0",
  "type": "module",
  "description": "Utility function",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    },
    "./date": {
      "import": "./dist/date.mjs",
      "require": "./dist/date.cjs",
      "types": "./date.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "author": "Mojie",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "prepare": "husky install",
    "dev": "npm run build -- --watch",
    "build": "rimraf dist && rollup -c",
    "release": "npx bumpp --commit --push --tag --no-verify",
    "docs": "typedoc",
    "docs:html": "npm run docs -- --out docs-html --plugin typedoc-plugin-missing-exports",
    "lint:eslint": "eslint --ext .ts . --fix",
    "lint:prettier": "prettier --write \"**/*.{ts,json,md}\"",
    "test": "vitest"
  },
  "bugs": {
    "url": "https://github.com/jayfongs/utils/issues"
  },
  "homepage": "https://github.com/jayfongs/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfongs/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "*.d.ts"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.13",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "dayjs": "^1.11.7",
    "esbuild": "^0.16.4",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.3",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "throttle-debounce": "^5.0.0",
    "typedoc": "^0.23.22",
    "typedoc-plugin-markdown": "^3.14.0",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.7"
  }
}
